import java.io;
import java.lang;
class Main {
    public static void main(String[] args) {
     String str1 = "pf=3122g;token=4563ghttt;";
     String[] sb=str1.split(";");
     for(String i:sb)
     {
         if(sb.startsWith("token="))
         {
            String token=sb.substring("token=".length());
            System.out.println("token is"+token);
         }
     }
    }
}
