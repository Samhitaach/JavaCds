
import java.util.HashMap;
import java.util.Map;
class Main {
    public static void main(String[] args) {
    int[] p={6,9,3,7,5,9,3};
  
    HashMap<Integer,Integer> m= new HashMap<Integer,Integer>();
    for(int i:p)
    {
    if(m.containsKey(i))
    {
    m.put(i,m.get(i)+1) 
    }
    else
     m.put(i,1);
    }
   
 System.out.println("Array counter"+m);     
}
